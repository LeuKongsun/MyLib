<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kongsun.mylib">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ereader_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ereader_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.StartupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"></activity>
        <activity android:name=".activity.BookNestedRCViewActivity"></activity>
        <activity android:name=".activity.CategoryActivity"></activity>
        <activity android:name=".activity.AccountActivity"></activity>
        <activity android:name=".activity.AboutUsActivity"></activity>
    </application>

</manifest>